{
    "name": "opdavies/drupal-vm-config-generator",
    "description": "A CLI tool for generating Drupal VM configuration files.",
    "require": {
        "symfony/console": "^3.0",
        "symfony/filesystem": "^3.0",
        "twig/twig": "^1.24"
    },
    "authors": [
        {
            "name": "Oliver Davies",
            "email": "oliver@oliverdavies.uk",
            "homepage": "https://www.oliverdavies.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "DrupalVmConfigGenerator\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin/"
    },
    "license": "MIT",
    "bin": [
        "bin/drupalvm-generate",
        "bin/generate.php"
    ]
}
