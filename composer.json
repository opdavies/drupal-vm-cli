{
    "name": "opdavies/drupal-vm-generator",
    "description": "A CLI tool for generating Drupal VM configuration files.",
    "require": {
        "symfony/console": "^3.0",
        "symfony/filesystem": "^3.0",
        "twig/twig": "^1.24",
        "knplabs/github-api": "^1.6",
        "guzzlehttp/guzzle": "^6.2",
        "herrera-io/phar-update": "^2.0"
    },
    "authors": [
        {
            "name": "Oliver Davies",
            "email": "oliver@oliverdavies.uk",
            "homepage": "https://www.oliverdavies.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "DrupalVmGenerator\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin/"
    },
    "license": "MIT",
    "bin": [
        "bin/drupalvm",
        "bin/drupalvm.php"
    ]
}
