{
    "name": "opdavies/drupal-vm-generator",
    "description": "A CLI tool for generating Drupal VM configuration files.",
    "keywords": ["vagrant", "drupal", "drupal-vm"],
    "homepage": "https://www.drupalvmgenerator.com",
    "require": {
        "guzzlehttp/guzzle": "^6.2",
        "knplabs/github-api": "^1.6",
        "symfony/console": "^3.0",
        "symfony/filesystem": "^3.0",
        "symfony/yaml": "^3.0",
        "twig/twig": "^1.24"
    },
    "authors": [
        {
            "name": "Oliver Davies",
            "email": "oliver@oliverdavies.uk",
            "homepage": "https://www.oliverdavies.uk"
        }
    ],
    "support": {
        "issues": "https://github.com/opdavies/drupal-vm-generator/issues",
        "source": "https://github.com/opdavies/drupal-vm-generator",
        "docs": "http://docs.drupalvmgenerator.com"
    },
    "autoload": {
        "psr-4": {
            "DrupalVmGenerator\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin/"
    },
    "license": "MIT",
    "bin": [
        "bin/drupalvm",
        "bin/drupalvm.php"
    ],
    "require-dev": {
        "herrera-io/phar-update": "^2.0",
        "friendsofphp/php-cs-fixer": "^1.11"
    },
    "suggest": {
        "geerlingguy/drupal-vm": "The main Drupal VM project."
    },
    "scripts": {
        "build": [
            "box build",
            "shasum drupalvm.phar | awk '{print $1}' > drupalvm.phar.version"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.9.x-dev"
        }
    }
}
