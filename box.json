{
    "chmod": "0775",
    "directories": [
        "templates",
        "src"
    ],
    "files": [
        "bin/drupalvm.php",
        "vendor/guzzle/guzzle/src/Guzzle/Http/Resources/cacert.pem",
        "vendor/herrera-io/phar-update/res/schema.json"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": ["Tests"],
            "in": "vendor"
        }
    ],
    "compactors": [
        "Herrera\\Box\\Compactor\\Php"
    ],
    "compression": "GZ",
    "git-version": "git_version",
    "main": "bin/drupalvm",
    "output": "drupalvm.phar",
    "stub": true
}
